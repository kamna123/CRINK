DO 200 I=1,N
	 XS1(I) = XS1(I+NV2)+XS3(I)
200	CONTINUE

DO 2210 I=1,N
	 XS1(I) = XS1(I+NVL)*XS3(I)+XS2(I)
	 
DO 1610 I=1,N
	 DO 1600 J=1,M
 	   XP1(I,J) = XP1(I+NV2,J+NV3)+XS3(I)
 	   
DO 1210 I=1,N
	 DO 1200 J=1,M
 	   XP1(J,I) = XP1(J+NPAR2,I)+XS3(J)
 	   
DO 1010 I=1,N
	 DO 1000 J=1,M
 	   XP1(I,J) = XP1(I+NV2,J-1)+XS3(I)
1000	 CONTINUE
1010	CONTINUE

DO 3400 I=1,NPARHD,NPAR2
	 XS1(I*I) = XS2(I*2+1)+XS3(3*I-3)
3400	CONTINUE

DO 2100 I=1,NPARHD
	 XS1(I*3+13) = XS2(I*2+1)+XS3(3*I-3)
2100	CONTINUE

DO 100 I=1,N-3
	   X(I) = X(I+M)*Y(I) + Z(I)
100	CONTINUE

 	    	  	 
